var Account = {
    deposit: lambda(self, amount) {
        self.balance = self.balance + amount;
    },
    withdraw: lambda(self, amount) {
        self.balance = self.balance - amount;
    }
};

var SavingsAccount = {
    proto: Account,
    compound: lambda(self) {
        self.balance = self.balance * (1 + self.interest);
